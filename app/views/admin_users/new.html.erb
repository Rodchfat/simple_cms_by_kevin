<strong><%= link_to "Back to Admin Users", admin_users_path %></strong>

<%= form_for(@admin_user) do |f| %>
<%= error_messages_for(f.object) %>
    <table summary="Admin User form fields">
  <tr>
    <th><%= f.label(:first_name, "First Name") %></th>
    <td><%= f.text_field(:first_name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:last_name, "Last Name") %></th>
    <td><%= f.text_field(:last_name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:email, "Email") %></th>
    <td><%= f.text_field(:email) %></td>
  </tr>
  <tr>
    <th><%= f.label(:username, "User Name") %></th>
    <td><%= f.text_field(:username) %></td>
  </tr>
  <tr>
    <th><%= f.label(:password, "Password") %></th>
    <td><%= f.text_field(:password) %></td>
  </tr>
  <tr>
  <div class = "form-button">
      <%= f.submit "Submit" %>
  </div>
  </tr>
  
  </table>
  <% end %>
